
<div class="container">
	<div class="jumbotron mt-3">
		<h1 class="mt-3">Member Information</h1>
		<div class="col-lg-3 col-md-6 mb-4">
			<div class="card">
				<img class="card-img-top" src="{{imageurl}}" alt="">
				<div class="card-body">
					<h4 class="card-title">{{name}}</h4>
					<p class="card-text">
						<h7 class="card-title">{{email}}</h7>
						<h7 class="card-title">{{phone}}</h7>
						<p>
							<h7 class="card-title">Hobbies: </h7>
							<ul>
								{{#each hobbies}}
									<li>{{this}}</li>
								{{/each}}
							</ul>
						</p>
					</p>
				</div>
			</div>
		</div>		
	</div>
</div>
	