<div class="container">
	<div class="jumbotron mt-3">
		<h1 class="mt-3 text-center">{{title}}</h1>
		<div class="col-lg-3 col-md-6 mb-4 mx-auto">
			<div class="card bg-dark text-light">
				<div class="card-body">
					<p class="card-text">
						<ul> 
							{{#each data}}
								<li>{{this.name}} || {{this.type}} || {{this.description}} || {{this.brand}} || {{this.price}}</li>
							{{/each}}
						</ul>
					</p>
				</div>
			</div>
		</div>		
	</div>
</div>
	